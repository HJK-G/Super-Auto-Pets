<Root>:
    FloatLayout:
        canvas.before:
            Color:
                rgba: 1, .349, 0, 1
            Rectangle:
                pos: self.pos
                size: self.size
        canvas.after:
            Rectangle:
                pos: 0, 400
                size: 100, 100
                source: 'resources/SAIPlogo.png'

        Button:
            text: "Start"
            pos: 150, 390
            size: 340, 100
            size_hint: None, None
            on_press: root.start_ai()

        AIVisualize:

<AIVisualize>:
    FloatLayout:
        Graph:
            pos: 3, 3
            size: 340, 360
            size_hint: None, None